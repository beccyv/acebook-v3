<h1>Listing Users</h1>

<table>
  <tr>
    <th>Username</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th></th>
  </tr>

  <% @users.each do |user| %>
    <tr>
      <td><%= user.username %></td>
      <td><%= user.first_name %></td>
      <td><%= user.last_name %></td>
      <td><%= link_to 'Show', user_path(user) %></td>
    </tr>
  <% end %>

  <%= link_to 'New user', new_user_path %>

</table>
